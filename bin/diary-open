#!/usr/bin/env zsh

. ~/.config/zsh/alias.zsh

n_loc='/mnt/c/notes/'

cd "$n_loc"

picocrypt enc*.zip.pcv
unzip enc*.zip

echo $pwd

fd 'encr' --maxdepth 1 -X rm


picocrypt 'Daily Notes' && rm 'Daily Notes' -rf

fd 'encr' --maxdepth 1 -x cp {} more/

fd encr more/ | sort | tail -n +11 | xargs -d '\n' rm -f







